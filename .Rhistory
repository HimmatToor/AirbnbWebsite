den <- read_csv("public/data/DEN.csv")
set.seed(123)
den_clean <- den %>%
filter(price <= 300,
number_of_reviews >= 5) %>%
sample_n(3000)
write_csv(ny_clean, "public/data/NY.csv")
write_csv(den_clean, "public/data/DEN.csv")
