library(dplyr)
library(readr)
ny <- read_csv("public/data/NY.csv")
ny <- ny %>%
filter(
price <= 300,                 # remove extreme outliers
number_of_reviews >= 5,       # active listings
availability_365 > 0,         # avoid inactive listings
room_type == "Entire home/apt" # reduce noise
)
